{
    "pages": [
        {
            "path": "/blocks",
            "name": "blocks",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "blocks",
                "hasInfoText": true,
                "storeNamespaces": ["block"],
                "managerGetter": "block/timeline",
                "infoTextGetter": "block/infoText",
                "columns": [
                    "height",
                    "age",
                    "transactions",
                    "totalFee",
                    "date",
                    "harvester"
                ],
                "mobileColumns": [
                    "height",
                    "age",
                    "transactions",
                    "totalFee"
                ]
            }
        },
        {
            "path": "/transactions",
            "name": "transactions",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "transactions",
                "storeNamespaces": ["transaction"],
                "managerGetter": "transaction/timeline",
                "hasFilter": true,
                "columns": [
                    "height",
                    "deadline",
                    "transactionHash",
                    "type",
                    "maxFee",
                    "signer",
                    "recipient"
                ],
                "mobileColumns": [
                    "height",
                    "transactionHash",
                    "fee",
                    "signer",
                    "recipient"
                ]
            }
        },
        {
            "path": "/accounts",
            "name": "accounts",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "accounts",
                "hasFilter": true,
                "managerGetter": "account/timeline",
                "storeNamespaces": ["account"],
                "mobileColumns": [
                    "address",
                    "importance",
                    "info"
                ]
            }
        },
        {
            "path": "/namespaces",
            "name": "namespaces",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "namespaces",
                "storeNamespaces": ["namespace"],
                "managerGetter": "namespace/timeline",
                "mobileColumns": [
                    "namespaceName",
                    "owneraddress",
                    "duration"
                ]
            }
        },
        {
            "path": "/mosaics",
            "name": "mosaics",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "mosaics",
                "storeNamespaces": ["mosaic"],
                "managerGetter": "mosaic/timeline",
                "mobileColumns": [
                    "mosaicId",
                    "mosaicAliasName",
                    "owneraddress"
                ]
            }
        },
        {
            "path": "/nodes",
            "name": "nodes",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "nodes",
                "storeNamespaces": ["node"],
                "managerGetter": "node/timeline",
                "columns": [
                    "index",
                    "version",
                    "roles",
                    "network",
                    "host",
                    "port",
                    "address",
                    "friendlyName"
                ],
                "mobileColumns": [
                    "index",
                    "roles",
                    "address",
                    "friendlyName",
                    "host"
                ]
            }
        }
    ]
}
