{
    "pages": [
        {
            "path": "/accounts",
            "name": "accounts",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "accounts",
                "hasFilter": true,
                "managerGetter": "account/timeline",
                "storeNamespaces": ["account"],
                "Fields": [
                    "address",
                    "balance",
                    "lastActivity",
                    "importance",
                    "accountAliasName"
                ],
                "mobileFields": [
                    "address",
                    "importance",
                    "info"
                ]
            }
        },
        {
            "path": "/namespaces",
            "name": "namespaces",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "namespaces",
                "storeNamespaces": ["namespace"],
                "managerGetter": "namespace/timeline",
                "Fields": [
                    "namespaceName",
                    "namespaceId",
                    "owneraddress",
                    "duration",
                    "approximateExpired"
                ],
                "mobileFields": [
                    "namespaceName",
                    "owneraddress",
                    "duration"
                ]
            }
        },
        {
            "path": "/mosaics",
            "name": "mosaics",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "mosaics",
                "storeNamespaces": ["mosaic"],
                "managerGetter": "mosaic/timeline",
                "Fields": [
                    "mosaicId",
                    "mosaicAliasName",
                    "owneraddress",
                    "supply",
                    "divisibility",
                    "relativeAmount",
                    "startHeight"
                ],
                "mobileFields": [
                    "mosaicId",
                    "mosaicAliasName",
                    "owneraddress"
                ]
            }
        },
        {
            "path": "/nodes",
            "name": "nodes",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "nodes",
                "storeNamespaces": ["node"],
                "managerGetter": "node/timeline",
                "Fields": [
                    "index",
                    "version",
                    "roles",
                    "network",
                    "host",
                    "port",
                    "address",
                    "friendlyName"
                ],
                "mobileFields": [
                    "index",
                    "roles",
                    "address",
                    "friendlyName",
                    "host"
                ]
            }
        }
    ]
}
