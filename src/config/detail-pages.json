{
    "pages": [
        {
            "path": "/account/:address",
            "name": "account-detail",
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Account Detail",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "dataGetter": "account/getAccountInfo",
                        "pagination": false,
                        "loadingGetter": "account/accountInfoLoading",
                        "errorGetter": "account/accountInfoError",
                        "errorMessage": "Account is unknown to the network"
                    },
                    {
                        "type": "Card",
                        "title": "Multisig Cosignatories",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "account/getAccountMultisigCosignatories",
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "account/accountInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Owned Mosaics",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "account/getMosaicList",
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "account/accountInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Owned Namespaces",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "account/getNamespaceList",
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "account/accountInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Metadata Entries",
                        "fullWidth": true,
                        "body": "TableListView",
                        "dataGetter": "account/getMetadataList",
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "account/accountInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Importance History",
                        "fullWidth": true,
                        "body": "TableListView",
                        "dataGetter": "account/getActivityBucketList",
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "account/accountInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Transactions",
                        "fullWidth": true,
                        "header": [
                            {
                                "type": "filter",
                                "filterOptionsGetter": "account/transactionFilterOptions",
                                "filterValueGetter": "account/transactionFilterValue",
                                "filterChangeAction": "account/setTransactionFilterValue"
                            }
                        ],
                        "body": "TableListView",
                        "dataGetter": "account/getTransactionList",
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "account/accountInfoLoading"
                    }
                ]
            }
        },
        {
            "path": "/block/:height",
            "name": "block-detail",
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Block Detail",
                        "fullWidth": true,
                        "header": [
                            {
                                "type": "Pagination",
                                "canFetchPrevious": true,
                                "canFetchNext": true,
                                "nextPageAction": "block/nextBlock",
                                "previousPageAction": "block/previousBlock"
                            }
                        ],
                        "body": "TableInfoView",
                        "dataGetter": "block/blockInfo",
                        "pagination": false,
                        "loadingGetter": "block/blockInfoLoading",
                        "errorGetter": "block/blockInfoError",
                        "errorMessage": "Block does not exist"
                    },
                    {
                        "type": "Card",
                        "title": "Block Transactions",
                        "fullWidth": true,
                        "body": "TableListView",
                        "dataGetter": "block/blockTransactionList",
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "block/blockInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Balance Change Receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/balanceChangeReceipt",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "block/blockInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Balance Transfer Receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/balanceTransferReceipt",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "block/blockInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Inflation Receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/inflationReceipt",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "block/blockInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Artifact Expiry Receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/artifactExpiryReceipt",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "block/blockInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Resolution Statement",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/resolutionStatement",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "block/blockInfoLoading"
                    }
                ]
            }
        },
        {
            "path": "/mosaic/:mosaicId",
            "name": "mosaic-detail",
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Mosaic Detail",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "dataGetter": "mosaic/getMosaicInfo",
                        "pagination": false,
                        "loadingGetter": "mosaic/mosaicInfoLoading",
                        "errorGetter": "mosaic/mosaicInfoError",
                        "errorMessage": "Mosaic does not exist"
                    },
                    {
                        "type": "Card",
                        "title": "Metadata Entries",
                        "fullWidth": true,
                        "body": "TableListView",
                        "dataGetter": "mosaic/getMetadataList",
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "mosaic/mosaicInfoLoading"
                    }
                ]
            }
        },
        {
            "path": "/namespace/:namespaceId",
            "name": "namespace-detail",
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Namespace Detail",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "dataGetter": "namespace/getNamespaceInfo",
                        "pagination": false,
                        "loadingGetter": "namespace/namespaceInfoLoading",
                        "errorGetter": "namespace/namespaceInfoError",
                        "errorMessage": "Namespace does not exist"
                    },
                    {
                        "type": "Card",
                        "title": "Namespace Level",
                        "fullWidth": true,
                        "body": "TableListView",
                        "dataGetter": "namespace/getNamespaceLevels",
                        "pagination": false,
                        "loadingGetter": "namespace/namespaceInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Metadata Entries",
                        "fullWidth": true,
                        "body": "TableListView",
                        "dataGetter": "namespace/getMetadataList",
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "namespace/namespaceInfoLoading"
                    }
                ]
            }
        },
        {
            "path": "/transaction/:transactionHash",
            "name": "transaction-detail",
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Transaction Info",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "dataGetter": "transaction/transactionInfo",
                        "pagination": false,
                        "loadingGetter": "transaction/transactionInfoLoading",
                        "errorGetter": "transaction/transactionInfoError",
                        "errorMessage": "Transaction does not exist"
                    },
                    {
                        "type": "Card",
                        "title": "Transaction Detail",
                        "fullWidth": false,
                        "body": "TableInfoView",
                        "dataGetter": "transaction/transactionDetail",
                        "pagination": false,
                        "loadingGetter": "transaction/transactionInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Mosaics",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "transaction/transferMosaics",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "transaction/transactionInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Aggregate Inner Transactions",
                        "fullWidth": true,
                        "body": "TableListView",
                        "dataGetter": "transaction/aggregateInnerTransactions",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "transaction/transactionInfoLoading"
                    },
                    {
                        "type": "Card",
                        "title": "Aggregate Cosignatures",
                        "fullWidth": true,
                        "body": "TableListView",
                        "dataGetter": "transaction/aggregateCosignatures",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "loadingGetter": "transaction/transactionInfoLoading"
                    }
                ]
            }
        }
    ]
}